{
  "action": {
    "default_icon": {
      "16": "img/icon_16.png",
      "32": "img/icon_32.png",
      "64": "img/icon_64.png"
    },
    "default_popup": "popup.html"
  },
  "author": "AdBlockify <support@adblockify.com>",
  "background": {
      "service_worker": "/js/background.js",
      "type": "module"
   },
  "declarative_net_request": {
    "rule_resources": [
    ]
  },
  "description": "Remove ads, block intrusive pop-ups, analytics trackers & malicious software.",
  "icons": {
    "16": "img/icon_16.png",
    "32": "img/icon_32.png",
    "64": "img/icon_64.png",
    "128": "img/icon_128.png"
  },
  "manifest_version": 3,
  "minimum_chrome_version": "105.0",
  "name": "AdBlockify - Ad blocker",
  "options_page": "dashboard.html",
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "activeTab",
    "declarativeNetRequest",
    "scripting",
    "storage"
  ],
  "short_name": "AdBlockify",
  "storage": {
    "managed_schema": "managed_storage.json"
  },
  "version": "1.0",
  "web_accessible_resources": []
}
